net: "TVG_CRFRNN_traintest_example.prototxt"

test_iter: 346
test_interval: 1000

display: 250

lr_policy: "fixed"
base_lr: 1e-13
momentum: 0.99

weight_decay: 0.0016

# The maximum number of iterations, DO NOT INCREASE THIS FURTHER
max_iter: 1000000000

# snapshot intermediate results
snapshot: 1000
snapshot_prefix: "/YOURPATH/crfrnnVOCresultmodels"

# solver mode: CPU or GPU
solver_mode: GPU
